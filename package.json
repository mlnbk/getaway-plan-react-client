    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.7"
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs",
      "^@Components/(.*)$": "<rootDir>/src/Components/$1",
      "^@Pages/(.*)$": "<rootDir>/src/Pages/$1",
      "^@Stores/(.*)$": "<rootDir>/src/Stores/$1",
      "^@types": "<rootDir>/src/types.ts",
